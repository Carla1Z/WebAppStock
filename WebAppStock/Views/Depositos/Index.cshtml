@{
}
<h1>Soy en Index de Depositos</h1>
<br />
<h3>Datos del deposito</h3>
@*<ul>
    <li><b>Id: </b> @Model.Id</li>
    <li><b>Nombre: </b> @Model.Nombre</li>
    <li><b>Capacidad: </b> @Model.Capacidad</li>
    <li><b>Dirección: </b> @Model.Direccion</li>
</ul>*@
<table class="table">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Nombre
            </th>
            <th>
                Capacidad
            </th>
            <th>
                Direccion
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var deposito in Model)
        {
            <tr>
                <td>@deposito.Id</td>
                <td>@deposito.Nombre</td>
                <td>@deposito.Capacidad</td>
                <td>@deposito.Direccion</td>
            </tr>
        }
    </tbody>
</table>