@using CodigoComun.Entities
@model IEnumerable<Stock>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
<h1>Soy en Index de Stocks</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                Articulo
            </th>
            <th>
                Deposito
            </th>
            <th>
                Cantidad
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var stock in Model)
        {
            <tr>
                <td>@stock.Id</td>
                <td>@(stock.Articulos != null ? stock.Articulos.Nombre : "N/A").<br /><i>Id: @stock.IdArticulo</i></td>
                <td>@(stock.IdDepositoNavigation != null ? stock.IdDepositoNavigation.Nombre : "N/A").<br /><i>Id: @stock.IdDeposito</i></td>
                <td>@stock.Cantidad</td>
            </tr>
        }
    </tbody>
</table>